{
  "expo": {
    "name": "GreenUpVermont",
    "description": "The Official App for Vermont's Green Up Day",
    "slug": "Green-Up-Vermont",
    "privacy": "public",
    "sdkVersion": "25.0.0",
    "primaryColor": "#EEEEEEE",
    "platforms": [
      "ios",
      "android"
    ],
    "githubUrl": "https://github.com/johnneed/GreenUpVermont",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/green-up-icon.png",
    "splash": {
      "image": "./assets/images/loading.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier" : "org.greenupvermont.app"
    },
    "android": {
      "package" : "org.greenupvermont.app",
      "versionCode" : 1,
      "permissions" : ["READ_CONTACTS", "ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"]
    },
    "androidStatusBar": {
      "barStyle" : "dark-content",
      "backgroundColor": "#EEEEEE"
    },
    "assetBundlePatterns": [
      "assets/images/*",
      "assets/fonts/*"
    ],
    "isRemoteJSEnabled" : true
  }
}
