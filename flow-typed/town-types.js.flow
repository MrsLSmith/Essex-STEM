// @flow
declare type TownLocation = {
    address: ?string;
    name: ?string;
    notes: ?string;
    coordinates: ?CoordinatesType;
}

declare class Town {
    id: ?string;
    name: ?string;
    celebrations: Array<Celebration>;
    description: ?string;
    notes: ?string;
    dropOffInstructions: ?string;
    dropOffLocations: Array<TownLocation>;
    pickupInstructions: ?string;
    pickupLocations: Array<TownLocation>;
    roadsideDropOffAllowed: ?boolean;
    created: ?Date;
    updated: ?Date;
}